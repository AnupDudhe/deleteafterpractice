apiVersion:  v1 
kind:  ReplicationController 
metadata:
   name: rc-flipkartapp
   labels:
     app: rc-flipkartapp 
spec:
  replicas: 3
  selector:
    #matchlabels:
    app: flipkartapp 
  template:
    metadata: 
      name: flipkarapp
      labels:
        app: flipkartapp 
    spec:
      containers:
      - name: nginx
        image: nginx:latest
        ports:
        - containerPort: 80